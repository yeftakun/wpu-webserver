    <h1>Hello World</h1>
    <p>Halo <%= nama %></p>

    <h4>Daftar Mahasiswa</h4>
    <!-- Melakukan looping dengan forEach terhadap array mahasiswa -->
     <% if (mahasiswa.length < 1) { %>
      <p>Data mahasiswa masih kosong</p>
     <% } %>
    <% mahasiswa.forEach(mhs => { %>
    <ul>
        <li>Nama: <%= mhs.nama %></li>
        <li>Email: <%= mhs.email    %></li>
    </ul>
    <% }) %>